  "notification_rules": [
    {
      "id": "critical-system-alerts",
      "name": "Critical System Alerts",
      "description": "Immediate notification for critical system issues",
      "conditions": {
        "priority": "critical",
        "notification_types": ["system_alerts", "critical_errors"]
      },
      "actions": {
        "target_groups": ["administrators"],
        "override_quiet_hours": true,
        "force_immediate": true,
        "escalation": {
          "enabled": true,
          "delay_minutes": 15,
          "escalate_to": ["sysadmin-001"]
        }
      }
    },
    {
      "id": "referee-assignment-changes",
      "name": "Referee Assignment Changes",
      "description": "Notify relevant parties about referee assignment changes",
      "conditions": {
        "notification_types": ["referee_assignments"],
        "change_categories": ["referee_added", "referee_removed", "referee_changed"]
      },
      "actions": {
        "target_groups": ["referee-management", "active-referees"],
        "personalize": true,
        "include_match_details": true
      }
    },
    {
      "id": "match-schedule-changes",
      "name": "Match Schedule Changes",
      "description": "Notify about significant match schedule changes",
      "conditions": {
        "notification_types": ["match_changes"],
        "change_categories": ["time_changed", "venue_changed", "match_cancelled"]
      },
      "actions": {
        "target_groups": ["referee-management", "active-referees"],
        "priority_boost": true,
        "include_change_summary": true
      }
    }
  ],
